{
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jquery": true
  },
  "rules": {
    "semi": [1, "always"],
    "quotes": [1, "single"],
    "no-console": 1,
    "no-unused-vars": 1
  },
  "plugins": [
      "react"
    ]
  }